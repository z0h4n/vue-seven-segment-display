{
  "name": "vue-seven-segment-display",
  "version": "2.1.0",
  "description": "A Seven Segment Display Component for Vue.js",
  "main": "dist/cjs.js",
  "module": "dist/esm.js",
  "unpkg": "dist/web.js",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config rollup.config.js --format cjs --file dist/cjs.js",
    "build:es": "rollup --config rollup.config.js --format es --file dist/esm.js",
    "build:unpkg": "rollup --config rollup.config.js --format iife --file dist/web.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/z0h4n/vue-seven-segment-display.git"
  },
  "keywords": [
    "vue",
    "vue-component",
    "vue component",
    "seven",
    "segment",
    "7segment",
    "7-segment",
    "7 segment",
    "sevensegment",
    "seven-segment",
    "seven segment",
    "component"
  ],
  "author": "z0h4n",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/z0h4n/vue-seven-segment-display/issues"
  },
  "homepage": "https://github.com/z0h4n/vue-seven-segment-display#readme",
  "dependencies": {},
  "devDependencies": {
    "rollup": "^1.17.0",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-terser": "^5.1.1",
    "rollup-plugin-vue": "^5.0.1",
    "vue-template-compiler": "^2.6.10"
  }
}
